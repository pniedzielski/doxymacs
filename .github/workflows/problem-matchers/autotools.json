{
    "problemMatcher": [
        {
            "owner": "autotools",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+):\\s+(.+):\\s+(.+)$",
                    "file": 1,
                    "line": 2,
                    "severity": 3,
                    "message": 4
                }
            ]
        }
    ]
}
